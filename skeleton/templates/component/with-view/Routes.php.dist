<?php

namespace {namespace};

use Simplon\Core\Utils\Routing\Routes;

class {name}Routes extends Routes
{
    const PATTERN_FOO = '/view/bar';

    /**
     * @return string
     */
    public static function toBar(): string
    {
        return self::toString(
            self::render(self::PATTERN_FOO)
        );
    }
}